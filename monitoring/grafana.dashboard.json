{
  "title": "Lang Graph Monitoring",
  "panels": [
    {
      "type": "timeseries",
      "title": "Calls per node (1m rate)",
      "targets": [
        {"expr": "sum(rate(node_calls_total[1m])) by (node)"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors per node (1m rate)",
      "targets": [
        {"expr": "sum(rate(node_errors_total[1m])) by (node)"}
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 latency per node",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(node_latency_seconds_bucket[5m])) by (le, node))"}
      ]
    }
  ]
}


